        function a_onClick(idClicked,maxClicked){


            for(var start=1;start<=maxClicked;start++)
            {var localId="li"+start;
                console.log(localId);

                if(start!=idClicked){
                    $("."+localId).hide();
                    $("#"+start).removeClass("active");
                }else
                {
                    $("."+localId).show();
                    $("#"+idClicked).addClass("active");

                }
            }


        }

	
